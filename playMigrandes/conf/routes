# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

#Aplicaciones del doctor
#Buscar paciente por id
GET		/paciente					controllers.PacienteController.buscarPacientePorId(id:Long)


GET     /getDoloresPaciente         controllers.DoctorController.getEpisodiosPaciente(idDoctor:Long,idPaciente:Long)

GET     /getDoloresTiempo			controllers.DoctorController.getEpisodiosTiempo(idDoctor:Long,idPaciente:Long)

GET     /getEpisodioDolor           controllers.DoctorController.getEpisodio(episodioId:Long)